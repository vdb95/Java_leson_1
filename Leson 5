public class Main {
    public void main(String[] args) {
        Employee employee1 = new Employee("Ivan", "director", "nmdqjwklndk", 89554225,300000,45);
        employee1.info();
    }
    class Employee{
        String name;
        String post;
        String email;
        int phoneNomber;
        int salary;
        int age;

        public Employee(String name, String post, String email, int phoneNomber, int salary, int age) {
            this.name = name;
            this.post = post;
            this.email = email;
            this.phoneNomber = phoneNomber;
            this.salary = salary;
            this.age = age;
        }

        public void info(){
            System.out.println(name + post + email + phoneNomber + salary + age);
        }
    }
}
